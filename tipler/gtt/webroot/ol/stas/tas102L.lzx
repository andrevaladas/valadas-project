								<text 		y="32" x="080"  height="21" text="Dimensión:" oninit="this.setAttribute('text', ('E'==lang ? 'Dimensión:' : ' Dimensão:') );"/>		   
								<lookup   	y="32" x="153" name="oid_Pneu_Dimensao" width="80" height="21" 
