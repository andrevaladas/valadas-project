						   });	
		cad.doRequest();
		tela.carregando.startAni();
	</method>

	<method name="selfPopulate2">
		cadDim.setAttribute("src","../../ControlerServlet?");
		cadDim.setQueryString({tela : "tas103",
										   ed : "Stas_Dimensao_SucateadoED",
