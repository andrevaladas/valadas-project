	<class name="tas102" extends="view" >

	<method name="selfPopulate">
		cad.setAttribute("src","../../ControlerServlet?");
		cad.setQueryString({tela : "tas103",
										   ed : "Stas_Pneu_SucateadoED",
			  							   rn : "Stas_Pneu_SucateadoRN",
						   			  	 acao : "L",
